{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "require-jsdoc": "off",
        "no-inner-declarations": "off",
        "max-len": "warn"
    },
    "plugins": [
        "cypress",
        "jest"
    ]
}
